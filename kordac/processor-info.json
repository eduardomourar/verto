{
    "boxed-text": {
        "class": "generic_container",
        "arguments": {
          "indented": {
            "required": false,
            "dependencies": []
          }
        },
        "template_name": "boxed-text",
        "template_parameters": {
          "indented": {
            "argument": "indented",
            "transform": "str.lower",
            "default": "no"
          },
          "text": {
            "argument": "content",
            "transform": null,
          }
        }
    },
    "button-link": {
        "class": "generic_tag",
        "arguments": {
          "link": {
            "required": true,
            "dependencies": []
          },
          "text": {
            "required": true,
            "dependencies": []
          },
          "file": {
            "required": false,
            "dependencies": []
          }
        },
        "template_parameters": {
          "link": {
            "argument": "link",
            "transform": "relative_file",
            "transform_condition": "lambda context: context['file'] == 'yes'"
          },
          "text": {
            "argument": "text",
            "transform": null,
          },
          "file": {
            "argument": "file",
            "transform": "str.lower",
            "default": "no"
          }
        }
    },
    "comment": {
        "class": "custom",
        "pattern" : "(^|\\n) *\\{comment [^\\}]+\\} *(\\n|$)",
        "arguments": {},
        "template_parameters": {}
    },
    "conditional": {
        "class": "custom",
        "arguments": {
          "condition": {
            "required": false,
            "dependencies": []
          },
          "if": {
            "required": false,
            "dependencies": ["condition"]
          },
          "elif": {
            "required": false,
            "dependencies": ["condition"]
          },
          "else": {
            "required": false,
            "dependencies": []
          }
        }
    },
    "glossary-link": {
        "class": "custom",
        "pattern": "\\{glossary-link ?(?P<args>[^\\}]*)}?(?P<text>.*?){glossary-link end\\}",
        "arguments": {
          "term": {
            "required": true,
            "dependencies": []
          },
          "reference-text": {
            "required": false,
            "dependencies": []
          }
        }
    },
    "heading": {
        "class": "custom",
        "pattern": "(^|\\n)(?P<level>#{1,6})(?!#+)\\s?(?P<header>.*?)\\s?#*(\\n|$)"
    },
    "iframe": {
        "class": "generic_tag",
        "arguments": {
          "link": {
            "required": true,
            "dependencies": []
          }
        },
        "template_name": "iframe",
        "template_parameters": {
          "link": {
            "argument": "link",
            "transform": null
          }
        }
    },
    "image": {
        "class": "custom",
        "pattern": "(^|\\n) *\\{image (?P<args>[^\\}]*)\\} *(\\n|$)",
        "arguments": {
          "file-path": {
            "required": true,
            "dependencies": []
          },
          "alt": {
            "required": false,
            "dependencies": []
          },
          "caption": {
            "required": false,
            "dependencies": []
          },
          "caption-link": {
            "required": false,
            "dependencies": []
          },
          "source": {
            "required": false,
            "dependencies": []
          },
          "alignment": {
            "required": false,
            "dependencies": []
          },
          "hover-text": {
            "required": false,
            "dependencies": []
          }
        }
    },
    "interactive": {
        "class": "custom",
        "pattern": "(^|\\n) *\\{interactive (?P<args>[^\\}]*)\\} *(\\n|$)",
        "arguments": {
          "name": {
            "required": true,
            "dependencies": []
          },
          "type": {
            "required": true,
            "dependencies": []
          },
          "text": {
            "required": false,
            "dependencies": []
          },
          "parameters": {
            "required": false,
            "dependencies": []
          },
          "thumbnail": {
            "required": false,
            "dependencies": []
          }
        }
    },
    "panel": {
        "class": "generic_container",
        "arguments": {
          "type": {
            "required": true,
            "dependencies": []
          },
          "title": {
            "required": true,
            "dependencies": []
          },
          "subtitle": {
            "required": false,
            "dependencies": []
          },
          "expanded": {
            "required": false,
            "dependencies": []
          }
        },
        "template_parameters": {
          "type": {
            "argument": "type",
            "transform": null
          },
          "title": {
            "argument": "title",
            "transform": null,
          },
          "subtitle": {
            "argument": "subtitle",
            "transform": null
          },
          "expanded": {
            "argument": "expanded",
            "transform": null
          },
          "content": {
            "argument": "content",
            "transform": null
          }
        }
    },
    "table-of-contents": {
        "class": "generic_tag",
        "arguments": {},
        "template_parameters": {}
    },
    "relative-link": {
        "class": "custom",
        "pattern": "\\[(?P<link_text>[^\\]]+)\\]\\((?!(https?|ftps?|mailto|news):)(?P<link_url>[^\\)]+)\\)"
    },
    "scratch": {
        "class": "custom",
        "scratch-compatibility": {
          "pattern": "(?P<fence>^(?:~{3,}|`{3,}))[ ]*scratch?[ ]*(hl_lines=(?P<quot>\"|')(?P<hl_lines>.*?)(?P=quot))?[ ]*}?[ ]*\n(?P<code>.*?)(?<=\n)(?P=fence)[ ]*$"
        }
    },
    "title": {
        "class": "custom",
        "pattern": "^#+ ?(.*)"
    },
    "video": {
        "class": "custom",
        "pattern": "(^|\\n) *\\{video (?P<args>[^\\}]*)\\} *(\\n|$)",
        "arguments": {
          "url": {
            "required": true,
            "dependencies": []
          }
        },
    }
}
